{
  "manifest_version": 2,
  "name": "ImageScraper",
  "description": "My first Chrome Extension",
  "version": "0.0.1",
  "author": "Joshua Kleckner <lokua@lokua.net>",
  "background": {
    "persistent": true,
    "scripts": [
      "lib/_.js",
      "lib/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "lib/assets/icon.png",
    "default_popup": "lib/popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "https://ajax.googleapis.com/"
  ]
}
